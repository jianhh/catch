<?xml version='1.0' encoding='UTF-8'?>
<weblogic-web-app xmlns="http://www.bea.com/ns/weblogic/121" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <weblogic-version>12.1</weblogic-version>
  <context-root>/</context-root>
<!--
  <prefer-web-inf-classes>true</prefer-web-inf-classes>
